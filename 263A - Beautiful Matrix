#include<bits/stdc++.h>
using namespace std;

int main(){

int arr[5][5],flag=0,k;
int i,j;
    for(int i=0; i<5; i++){
         for(int j=0;j<5;j++){
                cin>>arr[i][j];

              if(arr[i][j]==1){

                k=abs(abs(i-2)+abs(j-2));
                 flag=1;
                 break;

              }
         }
    }
    if(flag==1){
        cout<<k<<endl;
    }

  return 0;
}
