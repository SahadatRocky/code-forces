#include<bits/stdc++.h>
using namespace std;
int main(){
     
     long long n,m,a,l,k,s;
       cin>>n>>m>>a;
        l=n/a;
        k=m/a;
        
        if(n%a !=0){
        l++;
        }
        if(m%a !=0){
        k++;
        }
        
        s=l*k;
       cout<<s<<endl;
        
return 0;
}
