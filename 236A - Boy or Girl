
#include<bits/stdc++.h>

using namespace std;

int main(){
	
	set<char>s;
	string a;
	int c=0,f=0;
	  cin>>a;
	 
	 for(int i=0;i<a.size();i++){
	 	 s.insert(a[i]);
	 	 
	 	}
	  
	  set<char> :: iterator it;
	  
	  for(it=s.begin(); it!=s.end(); it++){
	  	     
	 
	  	     c++;	
	  	     	
	         }
	         
	         if(c%2==0)
	  	     {
	  	     
	  	     	cout<<"CHAT WITH HER!"<<endl;
	         	
	         	}
	         	else{
	         		cout<<"IGNORE HIM!"<<endl;
	         		}
	    
	
  	return 0;

}



