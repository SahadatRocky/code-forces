#include<bits/stdc++.h>
using namespace std;

int min(int a,int b,int z){
	
	  if(a<b && a<z){
	  	return a;
	  	
	  	}
	  	else if(b<a && b<z){
	  		return b;
	  		}
	  		else{
	  			return z;
	  			}
	
	
	}

int main()
{
	int n,k,l,c,d,p,nl,np;
	cin>>n>>k>>l>>c>>d>>p>>nl>>np;
	
	int kl=k*l;
	int a=kl/nl;
	int b=c*d;
	int z=p/np;
	
	int ss=min(a,b,z);
	cout<<ss/n;
     
    return 0;
}
