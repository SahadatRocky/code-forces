#include<bits/stdc++.h>
using namespace std;

int main(){
    
    string s1;
    string s2;
   vector<char>p;
    getline(cin,s1);
    getline(cin,s2);
    
    int len =s1.size();
    
    for(int i=0;i<len;i++){
if((s1[i]=='0' && s2[i]=='0')||(s1[i]=='1' && s2[i]=='1')){
  
          p.push_back('0');
    	  	}
    	  	else{
    	   
          p.push_back('1');
    	  		
    	  		}
    	}
    	
    	vector<char>:: iterator itr;
    	for(itr=p.begin(); itr!=p.end(); itr++){
    		
    		cout<<*itr;
    		}
    		cout<<endl;
    	
  // cout<<p<<endl; 
    return 0;
}
