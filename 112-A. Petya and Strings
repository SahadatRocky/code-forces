#include <bits/stdc++.h>

using namespace std;
 int main(){    
     string s1,s2;
     
     int f=0;
    getline( cin,s1 );
     getline( cin,s2 );
     
    for( int i=0; i<s1.size(); i++ ){
    	 s1[i] = tolower( s1[i] );
    	 s2[i] = tolower( s2[i] );
    	 
    	}
    		
    		if(s1 < s2){
    			f=-1;
    			
    			}
    		 else if(s1 > s2){
    			  
    			  f=1;
    		}
    		else{
    			  f=0;
    			
    			}
	    cout<<f<<endl;

    return 0;

}

