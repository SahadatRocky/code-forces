
#include<bits/stdc++.h>

using namespace std;


int main(){

	    int n;
	    cin>>n;
	    int f=0;
	    while(n--){
	    	int a;
	    	  cin>>a;
	    	  if(a==1){
	    	  	f=1;
	    	  	}
	    	}
	    	
	    	
	    	if(f==1){
	    		cout<<"-1"<<endl;
	    		}
	    		else{
	    			 cout<<"1"<<endl;
	    			
	    			}
	    		
	return 0;

}



