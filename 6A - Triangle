#include<bits/stdc++.h>
using namespace std;

int main(){
	int i=1;
	vector<int> v;
	 while(i<=4){
	 	int value;
	 	cin>>value;
	 	v.push_back(value);
	 	i++;
	 	}
  sort(v.begin(),v.end());
  
        if(v[0]+v[1]>v[2] || v[1]+v[2]>v[3]){
        	cout<<"TRIANGLE"<<endl;
        	}
    else if(v[0]+v[1]==v[2] || v[1]+v[2]==v[3]){
        	cout<<"SEGMENT"<<endl;
        	
        	}
        	else{
        		cout<<"IMPOSSIBLE"<<endl;
        		
        		}
   	 
return 0;
}
