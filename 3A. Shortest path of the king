#include<bits/stdc++.h>
using namespace std;

int main(){
	
	
	string s,v;
    
cin>>s>>v;
	
cout<<max( abs( s[0]-v[0] ) , abs( s[1]-v[1] ) )<<endl;
	
	while(s!=v){
		if(s[0]<v[0]){
			cout<<"R";
			s[0] += 1;
			
			}
			else if(s[0]>v[0]){
				cout<<"L";
				   s[0] -= 1;
				}
		
		 if(s[1]<v[1]){
		 	cout<<"U";
			s[1] += 1;
			
			}
			
			else if(s[1]>v[1]){
				 cout<<"D";
				 s[1] -= 1;
				}
		
		cout<<endl;
		}
	
return 0;
}
